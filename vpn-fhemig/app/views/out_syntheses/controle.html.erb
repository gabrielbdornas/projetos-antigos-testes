<div class="container">
  <div class="container">
    <br>
    <br>
    <h1>Controle total de vpns necessárias - Lista Carlotti</h1>
    <br>
    <br>
      <table class="table">
      <thead>
        <tr>
          <th scope="col">Estação</th>
          <th scope="col">Nome</th>
          <th scope="col">Ip Camg</th>
          <th scope="col">Host</th>
          <th scope="col">Patrimônio</th>
          <th scope="col">Mac</th>
          <th scope="col">Observação</th>
        </tr>
      </thead>
        <% @vpns.each do |import| %>
          <tbody>
            <tr>
              <th scope="row"><%= import.estacao %></th>
              <td><%= import.nome %></td>
              <td><%= import.ip %></td>
              <td><%= import.host_name %></td>
              <td><%= import.patrimonio %></td>
              <td><%= import.mac %></td>
              <td><%= import.observacao %></td>
            </tr>
          </tbody>
      <% end %>
      </table>
  </div>
</div>

